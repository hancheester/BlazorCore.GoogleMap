<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>Han Chee Tan</Authors>
    <PackageTags>blazor;jsinterop;javasript;google map;</PackageTags>
    <Version>$(VersionPrefix)</Version>
    <VersionPrefix>1.2.3</VersionPrefix>
    <RepositoryType>git</RepositoryType>
    <Copyright>©2024 Han Chee Tan</Copyright>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <Description>Blazor component that renders and manages Google map.</Description>
    <RepositoryUrl>https://github.com/hancheester/BlazorCore.GoogleMap</RepositoryUrl>
  </PropertyGroup>


  <ItemGroup>
    <SupportedPlatform Include="browser" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="BlazorCore.JSInterop" Version="1.0.3" />
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="6.0.36" />
  </ItemGroup>

</Project>
